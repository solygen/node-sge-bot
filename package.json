{
  "name": "node-sge-bot",
  "version": "1.1.0",
  "description": "eintracht frankfurt twitter news bot",
  "main": "bin/bot.js",
  "scripts": {
    "start": "node bin/bot.js",
    "stop": "echo '{}' > data/storage.json && echo '{}' > data/statistics.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "bitly": "2.0.0",
    "debug": "2.2.0",
    "deferred": "0.7",
    "feedparser": "1.1.4",
    "jsonfile": "2.3.1",
    "lodash": "4.13",
    "osmosis": "0.1.3",
    "request": "2.72.0",
    "twit": "2.2.4"
  },
  "engines": {
    "node": ">=4.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/solygen/node-sge-bot.git"
  },
  "preferGlobal": "true",
  "bin": {
    "bot": "bin/bot.js"
  },
  "keywords": [
    "service", "bot", "twitter", "news"
  ],
  "author": "Frank Paczynski <frank@solygen.de> (https://github.com/solygen)",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/solygen/node-sge-bot/issues"
  },
  "homepage": "https://github.com/solygen"
}
